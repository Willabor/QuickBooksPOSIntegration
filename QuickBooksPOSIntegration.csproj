<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net472</TargetFramework>
    <Nullable>disable</Nullable>
    <!-- If QuickBooks POS is 32-bit, force x86 build -->
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>

   <ItemGroup>
    <!-- Add the Microsoft.CSharp reference -->
    <Reference Include="Microsoft.CSharp" />
    
    <Reference Include="Interop.QBPOSFC4.Full">
      <HintPath>libs\Interop.QBPOSFC4.Full.dll</HintPath>
      <Private>true</Private>
    </Reference>
  </ItemGroup>

</Project>
